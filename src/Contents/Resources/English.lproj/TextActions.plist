<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>TEATextActions</key>
	<dict>
		<key>Emphasize</key>
		<dict>
			<key>action</key>
			<string>TEAWrapText</string>
			<key>options</key>
			<dict>
				<key>format</key>
				<string>html</string>
				<key>prefix</key>
				<string>em</string>
				<key>query_user</key>
				<false/>
				<key>suffix</key>
				<string>/em</string>
				<key>wrap_lines</key>
				<false/>
			</dict>
			<key>shortcut</key>
			<string>^i</string>
			<key>submenu</key>
			<string>Formatting</string>
		</dict>
		<key>Expand Abbreviation</key>
		<dict>
			<key>action</key>
			<string>TEASnippetWithWord</string>
			<key>options</key>
			<dict>
				<key>alpha_numeric</key>
				<true/>
				<key>bidirectional</key>
				<true/>
				<key>extra_characters</key>
				<string>_-#.&gt;+*:$!@</string>
				<key>mode</key>
				<string>zen</string>
				<key>snippet</key>
				<string>&lt;$SELECTED_TEXT&gt;$0&lt;/$WORD&gt;</string>
			</dict>
			<key>shortcut</key>
			<string>^,</string>
		</dict>
		<key>Indent New Line</key>
		<dict>
			<key>action</key>
			<string>TEAInsertSnippet</string>
			<key>options</key>
			<dict>
				<key>maintain_selection</key>
				<true/>
				<key>snippet</key>
				<string>
	$SELECTED_TEXT
</string>
			</dict>
			<key>shortcut</key>
			<string>^$
</string>
		</dict>
		<key>Insert Linebreak</key>
		<dict>
			<key>action</key>
			<string>TEAWrapText</string>
			<key>options</key>
			<dict>
				<key>format</key>
				<string>text</string>
				<key>prefix</key>
				<string></string>
				<key>query_user</key>
				<false/>
				<key>suffix</key>
				<string>&lt;br /&gt;</string>
				<key>wrap_lines</key>
				<false/>
			</dict>
			<key>shortcut</key>
			<string>^
</string>
		</dict>
		<key>Strong</key>
		<dict>
			<key>action</key>
			<string>TEAWrapText</string>
			<key>options</key>
			<dict>
				<key>format</key>
				<string>html</string>
				<key>prefix</key>
				<string>strong</string>
				<key>query_user</key>
				<false/>
				<key>suffix</key>
				<string>/strong</string>
				<key>wrap_lines</key>
				<false/>
			</dict>
			<key>shortcut</key>
			<string>^b</string>
			<key>submenu</key>
			<string>Formatting</string>
		</dict>
		<key>Trim Line(s)</key>
		<dict>
			<key>action</key>
			<string>TEATrim</string>
			<key>options</key>
			<dict>
				<key>alternate</key>
				<string>line</string>
				<key>input</key>
				<string>selected_lines</string>
				<key>respect_indent</key>
				<true/>
				<key>trim</key>
				<string>both</string>
			</dict>
			<key>shortcut</key>
			<string></string>
		</dict>
		<key>Wrap Selected Lines In Tag</key>
		<dict>
			<key>action</key>
			<string>TEAWrapText</string>
			<key>options</key>
			<dict>
				<key>format</key>
				<string>html</string>
				<key>prefix</key>
				<string>li</string>
				<key>query_user</key>
				<true/>
				<key>suffix</key>
				<string>/li</string>
				<key>wrap_lines</key>
				<true/>
			</dict>
			<key>shortcut</key>
			<string>@$^W</string>
		</dict>
		<key>Wrap Selection In Link</key>
		<dict>
			<key>action</key>
			<string>TEAWrapInLink</string>
			<key>options</key>
			<dict>
				<key>fallback</key>
				<string>http://</string>
				<key>snippet</key>
				<string>&lt;a href="$URL"&gt;$SELECTED_TEXT&lt;/a&gt;</string>
			</dict>
			<key>shortcut</key>
			<string>^$L</string>
		</dict>
		<key>Wrap Selection In Tag</key>
		<dict>
			<key>action</key>
			<string>TEAWrapText</string>
			<key>options</key>
			<dict>
				<key>format</key>
				<string>html</string>
				<key>prefix</key>
				<string>p</string>
				<key>query_user</key>
				<true/>
				<key>suffix</key>
				<string>/p</string>
				<key>wrap_lines</key>
				<false/>
			</dict>
			<key>shortcut</key>
			<string>^$W</string>
		</dict>
	</dict>
</dict>
</plist>
